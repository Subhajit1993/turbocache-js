{
  "name": "@brewedbytes/turbocache-js",
  "version": "1.0.0-alpha.3",
  "description": "High-performance, intelligent caching library for TypeScript/NestJS microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "cache",
    "caching",
    "redis",
    "nestjs",
    "typescript",
    "decorator",
    "microservices",
    "performance"
  ],
  "author": "Platform Engineering Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Subhajit1993/turbocache-js"
  },
  "dependencies": {
    "cacheable": "^1.5.3",
    "keyv": "^4.5.4",
    "reflect-metadata": "^0.1.14"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/testing": "^10.3.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@keyv/redis": "^2.8.4",
    "ioredis": "^5.3.2",
    "prom-client": "^15.1.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
